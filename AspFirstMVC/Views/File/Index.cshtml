@using AspFirstMVC.Models.ViewModels
@model UploadModel

<p>@ViewBag.message</p>

@Html.ValidationSummary(false, "", new { @class = "text-danger" })

<div class="text-center">
    <form asp-controller="File" asp-action="Upload1" enctype="multipart/form-data" method="post">
        <dl>
            <dt>
                <label>Archivo</label>
            </dt>
            <dd>
                <input type="file" asp-for=MyFile />
            </dd>
        </dl>
        <input type="submit" value="Upload" />
    </form>
</div>

<div>
    <a href="/File/Download1/1005">Descargar de BD</a><br />
    <a href="/File/Download2/1006">Descargar de Disco</a> <br />
    <a href="/File/Download3/1007">Descargar de S3</a>
</div>